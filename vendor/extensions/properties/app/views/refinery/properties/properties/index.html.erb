<% content_for :body do %>
  <ul id="properties">
    <% @properties.each do |property| %>
      <li>
        <%= link_to property.lot_name, refinery.properties_property_path(property) %>
      </li>
    <% end %>
  </ul>
<% end %>

<div id="property_index" class ="refinerycms-properties">
  <%= render '/refinery/content_page' %>
</div>
